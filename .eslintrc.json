{
    "parser": "@typescript-eslint/parser", 
    "extends": [
      "plugin:react/recommended",
      "plugin:@typescript-eslint/recommended",
      "google"
    ],
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "ecmaVersion": 2021
    },
    "rules": {
      "react/prop-types": "off",
      "@typescript-eslint/no-empty-interface": 0
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    }
  }